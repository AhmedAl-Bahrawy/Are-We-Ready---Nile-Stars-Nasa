    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.19",
    "@react-three/postprocessing": "^2.16.2",
    "@react-three/rapier": "^1.4.0",
    "@use-gesture/react": "^10.3.1",
    "axios": "^1.6.7",
    "gsap": "^3.12.5",
    "leaflet": "^1.9.4",
    "leva": "^0.9.35",
    "maath": "^0.10.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.0.0",
    "three": "^0.160.0",
